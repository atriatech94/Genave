<?xml version='1.0' encoding='utf-8'?>
<widget id="ir.atriatech.genave" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>بازار گناوه</name>
    <description>
        بازار گناوه
    </description>
    <author email="info@atriatech.ir" href="http://atriatech.ir">
        Atriatech Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="CameraUsesGeolocation" value="false" />
    <preference name="iosPersistentFileLocation" value="Library" />
    <preference name="orientation" value="portrait" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="deployment-target" value="10.0" />
    <preference name="StatusBarOverlaysWebView" value="true" />
</widget>
